{
  "name": "Truth-o-meter",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Upvote and downvote pages according to truth levels.",
  "icons": { "128": "icon_128.png" },
  "permissions": ["tabs", "<all_urls>"],
  "web_accessible_resources": [
    "./public/img/*.png"
    ],
  "browser_action": {
    "default_title": "",
    "default_icon": "icon_128.png",
    "default_popup": "./public/index.html",
    "popup": "./public/index.html"
  }
}
